{
  "name": "analytics-admin",
  "version": "1.0.0",
  "description": "openhn analytics-admin-server",
  "author": "openhn",
  "private": true,
  "license": "Apache License 2.0",
  "scripts": {
    "bootstrap": "sh scripts/bootstrap.sh",
    "up": "scripty",
    "stop": "scripty",
    "restart": "scripty",
    "docker:up": "docker-compose -f docker-compose-dev.yml up",
    "docker:stop": "docker-compose -f docker-compose-dev.yml stop",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:prod": "NODE_ENV=prod node dist/main",
    "start:test": "NODE_ENV=test node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "scripty": {
    "path": "./scripts"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.2",
    "@liaoliaots/nestjs-redis": "^9.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/body-parser": "^1.19.3",
    "@types/crypto-js": "^4.1.2",
    "@types/jsonwebtoken": "^9.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "deepmerge-ts": "^4.3.0",
    "file-stream-rotator": "^0.6.1",
    "ioredis": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.3",
    "mysql2": "^3.6.1",
    "nestjs-pino": "^3.1.2",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "scripty": "^2.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
